.con{
  width:100%;
  height:910rpx;
  border-top:1rpx solid #ededed;
  background:white;
}
page {
  background-color: #f4f5f7;
}
.con .logo{
  width:100%;
  height:300rpx;
  display:flex;
  align-items:center;
  flex-direction: column;
}
.con .logo image{
  width:140rpx;
  height:140rpx;
  border-radius:50%;
  margin:50rpx 0rpx 30rpx;
}
.con .logo text{
  font-size:28rpx;
}
.selectTime{
  width:100%;
  height:500rpx;
  margin-bottom:10rpx;
}
.times{
  height:100rpx;
  width:100%;
}
.times image{
  width:42rpx;
  height:42rpx;
  margin:25rpx 25rpx 0rpx;
}
.times .btn1{
  width:40rpx;
  height:40rpx;
  border-radius:50%;
  background:white;
  border:3rpx solid #d2d2d2;
  color:#d2d2d2;
  display:inline-block;
  text-align:center;
  line-height:40rpx;
  margin-top:25rpx;
  margin-right:15rpx;
}
.num{
  color:#b4a9a9;
  margin-right:15rpx;
}
.btns{
  float:right;
  line-height:95rpx;
  margin-right:30rpx;
}
.show{
  color:#0c4aff
}