/* components/my-number/my-number.wxss */
.content{
  width: 100%;
  padding:20rpx 29rpx;
  border-bottom:1px solid #EDEDED;
}
.content1{
  width: 100%;
  padding:41rpx 29rpx;
}
.weightText{
  display: inline-block;
  width:8rpx;
  height:35rpx;
  background:rgba(28,82,240,1);
  border-radius:4px;
}
.weightText1{
  width:266rpx;
  height:45rpx;
  font-size:32rpx;
  font-family:PingFangHK-Semibold,PingFangHK;
  font-weight:600;
  color:rgba(51,51,51,1);
  line-height:45rpx;
  letter-spacing:1rpx;
  padding-left: 10rpx;
}
.bedSize{
  width:38rpx;
  height:41rpx;
}
.rightF{
  float: right;
  padding-right: 90rpx;
  line-height: 50rpx;
  position: relative;
  font-size:32rpx;
  font-family:PingFangHK-Medium,PingFangHK;
  font-weight:500;
  color:rgba(28,82,240,1);
  line-height:45rpx;
  letter-spacing:1rpx;
}
.rightF text{
  display: inline-block;
  text-align: center;
  width:38rpx;
  height:38rpx;
  background:rgba(255,255,255,1);
  border-radius: 50%;
  line-height: 38rpx;
  color:gray ;
  border:1px solid gray;
}
.rightF text:nth-child(1){
  margin-right: 20rpx;
  margin-top:-10rpx;
}
.rightF text:nth-child(2){
  margin-left: 20rpx;
}
.lingText{
  position: relative;
  top:-5rpx;
  left: 10rpx;
  width:133rpx;
height:45rpx;
font-size:32rpx;
font-family:PingFangHK-Regular,PingFangHK;
font-weight:400;
color:rgba(51,51,51,1);
line-height:45rpx;
letter-spacing:1rpx;
}
/* 间隔 */
.interval{
  width: 100%;
  height:22rpx;
  background:rgba(244,245,247,1);
}
