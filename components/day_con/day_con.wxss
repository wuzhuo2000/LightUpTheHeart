.early{
  width:100%;
  height:223rpx;
  border-top:1rpx solid #ededed;
  margin-bottom:20rpx;
  background:white;
  display:flex;
}
.early image{
  width:75rpx;
  height:75rpx;
  border-radius:50%;
  margin:22.5rpx;
  margin-top:50rpx;
}
.early .item{
  padding-top:10rpx;
  align-items:center;
  flex-grow:1;
  display:flex;
  flex-direction:column;
}
.early text{
  font-size:28rpx;
  text-align:center;
  color:#666;
  font-family:'苹方-港 常规体';
}
.early .item text.black{
  color:black;
  font-weight:600;
}